<?xml version="1.0" encoding="UTF-8"?>
<xct:Popup xmlns:xct="http://xamarin.com/schemas/2020/toolkit" 
           xmlns="http://xamarin.com/schemas/2014/forms" 
           xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
           x:Class="DoAn_IE307_N11.Views.TransactionPopup"
           VerticalOptions="Center">

    <StackLayout>

        <StackLayout.Resources>
            <Style TargetType="{x:Type Button}" BasedOn="{StaticResource TransparentButton}" />
        </StackLayout.Resources>
        
        <Button Text="Khoảng thời gian" Clicked="TimeInterval_Clicked" />
        <Button Text="Xem theo giao dịch" />
        <Button Text="Điều chỉnh số dư" />
        <Button Text="Sửa ví" />
        
    </StackLayout>

</xct:Popup>